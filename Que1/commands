Image Creation:    docker build -t spring-boot-docker-gl.jar

Run Image:         docker run -p 9090:8080 spring-boot-docker-gl.jar

tag image:        docker tag spring-boot-docker-gl.jar jayantukey/spring-boot-docker-gl.jar

Pushing image:    docker push spring-boot-docker-gl.jar jayantukey/spring-boot-docker-gl.jar

Pull image:       docker pull spring-boot-docker-gl.jar jayantukey/spring-boot-docker-gl.jar

run image:        docker run -p 9090:8080 jayantukey/spring-boot-docker-gl.jar
